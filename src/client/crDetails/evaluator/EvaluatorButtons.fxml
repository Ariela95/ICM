<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import de.jensd.fx.glyphs.fontawesome.FontAwesomeIconView?>
<?import de.jensd.fx.glyphs.materialdesignicons.MaterialDesignIconView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" prefHeight="482.0" prefWidth="235.0" spacing="15.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.crDetails.evaluator.EvaluatorButtons">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <children>
      <Button fx:id="requestPhaseTimeButton" contentDisplay="RIGHT" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#showRequestTimeDialog" prefHeight="35.0" style="-fx-background-radius: 10;" text="Request Phase Time">
         <font>
            <Font size="14.0" />
         </font>
         <graphic>
            <MaterialDesignIconView glyphName="ALARM" size="20" />
         </graphic>
      </Button>
      <JFXButton fx:id="phaseTimeRequestInfo" alignment="TOP_LEFT" onAction="#moreInformation1Event" prefHeight="26.0" prefWidth="215.0" text="Why this button is disable?" textFill="#1b0eae" underline="true">
         <graphic>
            <FontAwesomeIconView fill="#3073d9" glyphName="EXCLAMATION_CIRCLE" size="20" text="" />
         </graphic>
         <font>
            <Font size="14.0" />
         </font>
      </JFXButton>
      <Button fx:id="createEvaluationReportButton" contentDisplay="RIGHT" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#showCreateEvaluationReportDialog" prefHeight="35.0" style="-fx-background-radius: 10;" text="Create Evaluation Report">
         <font>
            <Font size="14.0" />
         </font>
         <graphic>
            <MaterialDesignIconView glyphName="CONTENT_PASTE" size="17" />
         </graphic>
      </Button>
      <JFXButton fx:id="createReportInfo" alignment="TOP_LEFT" onAction="#moreInformation2Event" prefHeight="26.0" prefWidth="215.0" text="Why this button is disable?" textFill="#1b0eae" underline="true">
         <graphic>
            <FontAwesomeIconView fill="#3073d9" glyphName="EXCLAMATION_CIRCLE" size="20" text="" />
         </graphic>
         <font>
            <Font size="14.0" />
         </font>
      </JFXButton>
      <JFXButton fx:id="returnRequestInfo" alignment="TOP_LEFT" onAction="#importantInfoEvent" prefHeight="26.0" prefWidth="215.0" text="important info" textFill="#1b0eae" underline="true">
         <graphic>
            <FontAwesomeIconView fill="#3073d9" glyphName="EXCLAMATION_CIRCLE" size="20" text="" />
         </graphic>
         <font>
            <Font size="14.0" />
         </font>
      </JFXButton>
   </children>
</VBox>
